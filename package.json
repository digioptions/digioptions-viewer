    "node-sass-json-importer": "^4.3.0",
    "os-browserify": "^0.3.0",
    "prop-types": "~15.7.2",
    "raven-js": "~3.27.0",
    "react": "~17.0.0",
    "react-bootstrap": "~1.4.0",
    "react-dom": "~17.0.0",
    "react-redux": "~7.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "rimraf": "~3.0.0",
    "sass-loader": "^10.0.0",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.1",
    "swiper": "git://github.com/nolimits4web/swiper.git#v6.0.4",
    "terser-webpack-plugin": "^4.0.0",
    "typeface-open-sans": "0.0.75",
    "typeface-roboto-condensed": "0.0.75",
    "web3": "1.3.3",
    "webpack": "^4.0.0",
