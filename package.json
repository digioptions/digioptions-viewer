{
  "name": "digioptions-viewer",
  "version": "0.2.5",
  "description": "User Driven Digital Option Markets based on Ethereum smart contracts",
  "license": "All rights reserved.",
  "private": true,
  "author": {
    "name": "digioptions.com",
    "url": "https://www.digioptions.com"
  },
  "scripts": {
    "serve": "ws --hostname 127.0.0.1 --port 8082",
    "clean-js": "rimraf dist/js/digioptions.*.js dist/js/digioptions.*.map",
    "clean": "rimraf dist && mkdir dist dist/js dist/css",
    "build": "npm run clean && npm run build-template && npm run build-js-css",
    "build-js-css": "npm run clean-js && NPM_PACKAGE_VERSION=$npm_package_version REACT_APP_VERSION=$npm_package_version webpack --config webpack.prod.js && mv dist/js/bundle.min.js.map dist/js/digioptions-viewer.bundle-$npm_package_version.min.js.map",
    "eslint": "eslint ./js/*.js ./tools/*.js",
    "build-template": "rimraf dist/*.html dist/template.* && REACT_APP_VERSION=$npm_package_version webpack --config webpack.template.js && rimraf dist/template*",
    "webpack-start": "npm run clean-js && npm run build-template && webpack serve --config webpack.dev.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "css/",
    "js/",
    "patches/",
    "tools/"
  ],
  "repository": {},
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "~7.12.0",
    "@babel/plugin-proposal-class-properties": "~7.12.0",
    "@babel/preset-env": "~7.12.0",
    "@babel/preset-react": "~7.12.0",
    "@babel/register": "~7.12.0",
    "babel-eslint": "~10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "~1.1.1",
    "babelify": "10.0.0",
    "bootstrap": "~4.5.3",
    "budo": "^11.6.3",
    "contenthash-signer-ens": "git+https://github.com/berlincode/contenthash-signer-ens.git",
    "core-js": "~3.8.0",
    "crypto-browserify": "^3.12.0",
    "d3": "~6.5.0",
    "digioptions-app": "file:../digioptions-repo/digioptions-app-latest.tgz",
    "digioptions-contracts.js": "git+https://github.com/berlincode/digioptions-contracts.js.git",
    "digioptions-tools.js": "git+https://github.com/berlincode/digioptions-tools.js.git",
    "eslint": "^7.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-react": "~7.22.0",
    "factsigner": "git+https://github.com/berlincode/factsigner.git",
    "file-loader": "^6.2.0",
    "history": "^5.0.0",
